{
  "name": "rue-route",
  "version": "0.0.1",
  "description": "rue - router extension",
  "main": "index.js",
  "engines": {
    "node": ">=6.11.0"
  },
  "keywords": [
    "rue",
    "rue framework",
    "ioc",
    "inversion of control",
    "di",
    "dependency injection"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "flow": "flow",
    "prepare": "npm run build",
    "build": "npm run build-babel && npm run build-flow",
    "build-babel": "npm run clean && babel ./lib --out-dir dist/ --copy-files",
    "build-flow": "flow-copy-source ./lib ./dist",
    "test": "npm run flow && jest --coverage",
    "test-watch": "npm run flow && jest --watch",
    "jscs": "jscs lib/**/*.js test"
  },
  "repository": {
    "url": "http://github.com/bemisguided/rue-route.git",
    "type": "git"
  },
  "author": "@bemisguided",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bemisguided/rue-route/issues"
  },
  "homepage": "https://github.com/bemisguided/rue-route#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.48.0",
    "flow-copy-source": "^1.2.0",
    "jest": "^20.0.4",
    "jscs": "^3.0.7",
    "q": "^1.5.0",
    "rimraf": "^2.6.1",
    "rue": "0.8.0"
  },
  "peerDependencies": {
    "rue": "0.8.0"
  },
  "files": [
    "index.js",
    "dist"
  ],
  "false": {},
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "lib/**.{js}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html"
    ]
  }
}
